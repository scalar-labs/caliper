{
  "caliper" : {
    "blockchain": "scalardl",
    "command" : {
      "start": "/bin/sh network/scalardl/scripts/up.sh docker-compose.yml 30",
      "end" : "/bin/sh network/scalardl/scripts/down.sh docker-compose.yml"
    }
  },
  "contract": { "path": "src/contract/scalardl" },
  "scalardl": {
    "network": {
      "client_properties": {
        "host": "localhost",
        "port": "50051",
        "cert_holder_id": "test",
        "cert_version": 1,
        "cert_path": "network/scalardl/certs/client.pem",
        "private_key_path": "network/scalardl/certs/client-key.pem",
        "tls_enabled": false
      }
    },
    "contracts": [
      {
        "id": "CreateAccount",
        "name": "com.example.contract.smallbank.CreateAccount",
        "fcn": "create_account",
        "path": "build/classes/java/main/com/example/contract/smallbank/CreateAccount.class"
      },
      {
        "id": "DepositChecking",
        "name": "com.example.contract.smallbank.DepositChecking",
        "fcn": "deposit_checking",
        "path": "build/classes/java/main/com/example/contract/smallbank/DepositChecking.class"
      },
      {
        "id": "TransactSavings",
        "name": "com.example.contract.smallbank.TransactSavings",
        "fcn": "transact_savings",
        "path": "build/classes/java/main/com/example/contract/smallbank/TransactSavings.class"
      },
      {
        "id": "WriteCheck",
        "name": "com.example.contract.smallbank.WriteCheck",
        "fcn": "write_check",
        "path": "build/classes/java/main/com/example/contract/smallbank/WriteCheck.class"
      },
      {
        "id": "SendPayment",
        "name": "com.example.contract.smallbank.SendPayment",
        "fcn": "send_payment",
        "path": "build/classes/java/main/com/example/contract/smallbank/SendPayment.class"
      },
      {
        "id": "Amalgamate",
        "name": "com.example.contract.smallbank.Amalgamate",
        "fcn": "amalgamate",
        "path": "build/classes/java/main/com/example/contract/smallbank/Amalgamate.class"
      },
      {
        "id": "QueryAccount",
        "name": "com.example.contract.smallbank.QueryAccount",
        "fcn": "query",
        "path": "build/classes/java/main/com/example/contract/smallbank/QueryAccount.class"
      }
    ]
  }
}
