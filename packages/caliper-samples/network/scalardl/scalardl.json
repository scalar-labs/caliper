{
  "caliper" : {
    "blockchain": "scalardl",
    "command" : {
      "start": "echo docker-compose -f network/scalardl/docker-compose.yml up -d",
      "end" : "echo docker-compose -f network/scalardl/docker-compose.yml down"
    }
  },
  "contract": { "path": "src/contract/scalardl" },
  "scalardl": {
    "network": {
      "client_properties": {
        "host": "localhost",
        "port": "50051",
        "cert_holder_id": "test",
        "cert_version": 1,
        "cert_path": "network/scalardl/certs/client.pem",
        "private_key_path": "network/scalardl/certs/client-key.pem",
        "tls_enabled": false
      }
    },
    "contracts": [
      {
        "id": "OpenAccount",
        "name": "com.example.contract.simple.OpenAccount",
        "fcn": "open",
        "path": "build/classes/java/main/com/example/contract/simple/OpenAccount.class"
      },
      {
        "id": "QueryAccount",
        "name": "com.example.contract.simple.QueryAccount",
        "fcn": "query",
        "path": "build/classes/java/main/com/example/contract/simple/QueryAccount.class"}
    ]
  }
}
